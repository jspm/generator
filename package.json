{
  "name": "@jspm/generator",
  "description": "Package Import Map Generation Tool",
  "license": "MIT",
  "version": "1.0.0-beta.2",
  "types": "dist/generator.d.ts",
  "scripts": {
    "tsc": "tsc -p .",
    "tsc-watch": "tsc -p . --watch",
    "rollup": "rollup -c && cp lib/generator.d.ts dist/generator.d.ts && mkdir -p dist/common && cp lib/common/log.d.ts dist/common/log.d.ts",
    "build": "npm run tsc && npm run rollup",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "node test/test.js",
    "test:browser": "node test/server.mjs"
  },
  "type": "module",
  "imports": {
    "#test/*": "./test/*.js"
  },
  "exports": {
    "test": "./lib/generator.js",
    "default": "./dist/generator.js"
  },
  "dependencies": {
    "es-module-lexer": "^0.4.1",
    "kleur": "^4.1.4",
    "make-fetch-happen": "^8.0.3",
    "mocha": "^9.0.0",
    "open": "^8.2.0",
    "sver": "^1.8.3"
  },
  "devDependencies": {
    "chalk": "^4.1.1",
    "rollup": "^2.44.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jspm/generator.git"
  },
  "keywords": [
    "jspm",
    "import maps",
    "es modules",
    "cdn",
    "package manager"
  ],
  "author": "Guy Bedford",
  "bugs": {
    "url": "https://github.com/jspm/generator/issues"
  },
  "homepage": "https://github.com/jspm/generator#readme"
}
